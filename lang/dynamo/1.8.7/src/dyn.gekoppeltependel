NOTE gekoppelte pendel
L    x1.k=x1.j+dt*v1.j
L    x2.k=x2.j+dt*v2.j
L    v1.k=v1.j+dt*(-d0/m1*x1.j-(d/m1)*(x1.j-x2.j))
L    v2.k=v2.j+dt*(-d0/m2*x2.j-(d/m2)*(x2.j-x1.j))
N    x1=a
N    x2=0
N    v1=0
N    v2=0
C    a=3
C    m1=2
C    m2=2
C    d0=9
C    d=2
C    dt=0.1
C    length=50
C    pltper=0.3
PLOT x1=1(-3,9)/x2=2(-9,3)

