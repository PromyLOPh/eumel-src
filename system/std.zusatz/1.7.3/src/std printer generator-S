forget ("std printer generator/S", quiet) ;
check off ;

ins ("minimal fonts routines"); 
ins ("std printer"); 
ins ("eumel printer");
ins ("elan lister");
ins ("printer/S");
 
PROC ins (TEXT CONST name): 
  fetch (name, archive);
  insert (name); 
  forget (name, quiet) 
END PROC ins;
 
