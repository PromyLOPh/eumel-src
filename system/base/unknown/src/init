PACKET a :

PROC out (TEXT CONST t) :
  EXTERNAL 60
ENDPROC out ;

PROC out text (TEXT CONST t, INT CONST typ) :
  INTERNAL 257 ;
  out (t)
ENDPROC out text ;

PROC out line (INT CONST typ) :
  INTERNAL 258 ;
  out (""13""10"")
ENDPROC out line ;

ENDPACKET a ; 
