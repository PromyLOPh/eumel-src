PACKET a :

PROC out (TEXT CONST t) :
  EXTERNAL 60
ENDPROC out ;

PROC out text (TEXT CONST t, INT CONST typ) :
  INTERNAL 257 ;
  IF typ = typ
    THEN out (t)
  FI
ENDPROC out text ;

PROC out line (INT CONST typ) :
  INTERNAL 258 ;
  IF typ = typ
    THEN out (""13""10"")
  FI
ENDPROC out line ;

ENDPACKET a ; 

