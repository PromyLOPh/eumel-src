enter outcode (13, 13);                          (* RETURN                *)
enter outcode (14,  0, csi + "m ");              (* END MARK              *)
enter outcode (15,  0, csi + "7m ");             (* BEGIN MARK            *)

enter outcode (27, 27);                          (* ESC                   *)

cursor logic (1,1,csi,";","H"); 
 
