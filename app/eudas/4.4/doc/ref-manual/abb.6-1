PROC pfeil (REAL CONST x spitze, y spitze) : 
 
  move (x spitze, y spitze); 
  draw (x spitze + 0.25, y spitze - 0.25); 
  draw (x spitze + 0.1, y spitze - 0.25); 
  draw (x spitze + 0.1, y spitze - 0.5); 
  draw (x spitze - 0.1, y spitze - 0.5); 
  draw (x spitze - 0.1, y spitze - 0.25); 
  draw (x spitze - 0.25, y spitze - 0.25); 
  draw (x spitze, y spitze) 
 
END PROC pfeil; 
 
 
